function (x, y, sigmaf = 1, l = 1, sigman = 0) 
{
    z <- sigmaf^2 * exp(-(1/(2 * l^2)) * (sum(x - y))^2)
    if (x == y) 
        z <- z + sigman^2
    z
  }
}
